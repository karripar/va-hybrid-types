{
    "name": "va-hybrid-types",
    "version": "1.0.0",
    "description": "Types for mediapalveluprojekti",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./dist/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "prepare": "npm run build"
    },
    "private": true,
    "author": "Karri Partanen, Elli Riskala, Samu Kangas, Odette Uzamukunda",
    "devDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "next": "^15.5.4",
        "react": "^19.2.0",
        "va-hybrid-types": "github:karripar/va-hybrid-types"
    }
}
